{
  "name": "react-native-router-flux",
  "version": "4.0.2",
  "description": "React Native Router using Flux architecture",
  "repository": {
    "type": "git",
    "url": "https://github.com/aksonov/react-native-router-flux"
  },
  "license": "MIT",
  "main": "src/index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "./node_modules/.bin/opencollective postinstall",
    "build": "./node_modules/.bin/babel src -d dist",
    "eslint": "eslint src/ __tests__/",
    "fix": "eslint src/ __tests__/ --fix",
    "test": "jest"
  },
  "resolutions": {
    "*/@babel/cli": "7.0.0-beta.47",
    "*/@babel/core": "7.0.0-beta.47",
    "*/@babel/code-frame": "7.0.0-beta.47",
    "*/babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "opencollective": "^1.0.3",
    "path-to-regexp": "^2.2.1",
    "prop-types": "^15.6.2",
    "react-navigation": "^2.11.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.47",
    "@babel/code-frame": "7.0.0-beta.47",
    "@babel/core": "7.0.0-beta.47",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.47",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.47",
    "@babel/preset-flow": "7.0.0-beta.47",
    "@types/react": "^16.4.11",
    "@types/react-native": "^0.56.9",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.0",
    "jest": "^23.4.1",
    "babel-preset-react-native": "^5.0.2",
    "react-test-renderer": "^16.4.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "estraverse-fb": "^1.3.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^5.0.0-beta.0",
    "react": "^16.4.1",
    "react-native": "^0.56.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/Example/",
      "<rootDir>/ReduxExample/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-native-router-flux",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
